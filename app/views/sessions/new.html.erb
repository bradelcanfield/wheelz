


<div class="holder">
  <h1>Log In</h1>
<%= form_tag '/login' do %>

  Email: <%= text_field_tag :email %>
  Password: <%= password_field_tag :password %>
  <%= submit_tag "Submit" %>
  <%end%><br>
  <div>
    <button id="signup-bttn"><%= link_to 'New user? Sign up now!', profile_new_path%></button><br>
  </div>
</div>
